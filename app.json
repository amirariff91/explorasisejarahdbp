{
  "expo": {
    "name": "Explorasi Sejarah",
    "slug": "explorasisejarahdbp",
    "version": "1.0.0",
    "description": "An interactive educational game to teach Malaysian history through gameplay",
    "orientation": "landscape",
    "icon": "./assets/images/icon.png",
    "scheme": "explorasisejarahdbp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "assetBundlePatterns": [
      "assets/audio/**/*",
      "assets/images/**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.amirariff.explorasisejarahdbp",
      "requiresFullScreen": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ],
        "UISupportedInterfaceOrientations": [
          "UIInterfaceOrientationLandscapeLeft",
          "UIInterfaceOrientationLandscapeRight"
        ],
        "UISupportedInterfaceOrientations~ipad": [
          "UIInterfaceOrientationLandscapeLeft",
          "UIInterfaceOrientationLandscapeRight"
        ]
      }
    },
    "android": {
      "package": "com.amirariff.explorasisejarahdbp",
      "screenOrientation": "landscape",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "statusBar": {
        "translucent": true,
        "backgroundColor": "transparent"
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/game/logo-dbp/logo-dbp.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/images/game/logo-dbp/logo-dbp.png",
            "backgroundColor": "#ffffff"
          }
        }
      ],
      "expo-secure-store",
      "expo-audio",
      "expo-asset",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f0b91514-8c96-4844-95a1-185dd39c28f1"
      }
    }
  }
}
